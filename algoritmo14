package main

import "fmt"

func main() {
	slice := []int{10, 20, 30, 40, 50, 60, 70, 80}

	var i int
	var j int
	fmt.Print("Digite o valor do primeiro elemento: ")
	fmt.Scan(&i)
	fmt.Print("Digite o valord o segundo elemento: ")
	fmt.Scan(&j)

	slice[i], slice[j] = slice[j], slice[i]

	fmt.Println(slice)
}
